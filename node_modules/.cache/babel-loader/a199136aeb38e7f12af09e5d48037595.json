around>\n        <v-toolbar-side-icon>\n            \n            <v-img class='logo' src='../assets/logo_home.svg'> \n            </v-img>\n     </v-toolbar-side-icon>\n     <!-- <v-spacer></v-spacer> -->\n     <div>\n        <v-btn @click=\"$router.push('/Card')\" v-show=\"elementVisible\" class=\"hideElement\" color=\"blue lighten-1\" dark> Confirmer le don </v-btn> \n    </div>\n  <div class=\"progress\">\n  <div class=\"progress-value\"></div>\n   \n</div>\n</v-layout>\n </v-toolbar>\n</nav>\n    <Media \n      :kind=\"'video'\"\n      :isMuted=\"(false)\"\n      :src=\"selectedLink\"\n      :autoplay=\"true\" \n      :controls=\"false\"\n      :loop=\"false\"\n      :ref=\"'fish'\"\n      :style=\"{width: '50vw', margin:'100px'}\"\n      @pause=\"handle()\"\n      @click=\"onClickVideo()\">\n      \n    >\n\n    \n    </Media>\n  </div>\n</template>\n\n<script >\nimport Media from '@dongido/vue-viaudio'\n\nexport default {\n  computed:{\n      campaigns(){\n        return this.$store.state.campaigns;\n      }\n      },\n  data () {\n      return{\n          elementVisible: false,\n          \nredir:'',\nselectedLink:''\n\n      }\n  },\n  components: {\n    Media\n  },\n  created () {\n    const idx = Math.floor(Math.random() * this.campaigns.length);\n    this.selectedLink = this.campaigns[idx].video_uri;\n    this.redir = this.campaigns[idx].redirect_uri;\n    setTimeout(() => this.elementVisible = true, 20000)\n  },\n\n  methods: {\n      onClickVideo () {\n          window.open(this.redir, \"_blank\")\n        // window.location.href = this.redir\n      }\n      \n   \n  }\n}\n\n\n\n\n\n</script>\n\n<style>\n#vid {\n  width: 100%;\n  text-align: center;\n}\n\n.logo {\n    margin-left:150px;\n}\n\n.progress {\n  background: #e3f2fd;\n  justify-content: flex-start;\n  border-radius: 10px;\n  align-items: center;\n  position: relative;\n  /* padding: 0 5px; */\n  display: flex;\n  height: 30px;\n  width: 200px;\n    margin-right:150px;\n      border-radius:15px;\n}\n\n.progress-value {\n  animation: load 20s linear forwards;\n  box-shadow: 0 10px 40px -10px #fff;\n  border-radius: 10px;\n  background: #42A5F5;\n  height: 30px;\n  width: 0;\n    border-radius:10px;\n}\n\n@keyframes load {\n  0% { width: 0; }\n  100% { width: 100%; }\n}\n\n.hideElement{\n    \n}\n\n\n\n\n</style>"],"sourceRoot":"src/components"}]}