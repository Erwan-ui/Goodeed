return this.$store.state.associations;\n      }\n\n    },\n    methods:{\n      addIdProject(projectId){\n        this.$store.commit('addId',projectId);\n      \n      },\n      increaseCounter(projectId){\n        this.$store.commit('increaseCounter',projectId);\n        \n      },\n      increment(){\n      this.$store.commit('increment')\n      },\n      \n}, \n  }\n</script>\n\n<style>\n\n.zoom:hover{\n  transform: scale(1.05)\n}\n\n.caro {\n  position: relative;\n}\nimg {\n  position: absolute;\n  height: 60px;\n  width: 60px;\n  top: 140px;\n  left: 20px;\n  z-index: 1000;\n  border-radius: 10px;\n}\n.mx-auto {\n  border-radius: 10px;\n  /* width:340px;\n  height:444px; */}\n\n.compteur{\n    font-family:gotham,Helvetica,sans-serif;\n    color:#89969f;\n    font-weight:400;\n    line-height:1;\n    font-size:17px;\n    float:left;\n    padding:3px;\n}\n\n.donat{\n    font-family:gotham,Helvetica,sans-serif;\n    color:#353e44;\n    font-weight:700;\n    line-height:1;\n    font-size:17px;\n    float:left;\n    padding:3px;\n    \n}\n\n\n\n</style>"],"sourceRoot":"src/components"}]}